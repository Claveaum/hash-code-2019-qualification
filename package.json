{
  "name": "hash-code-2019-qualification",
  "version": "1.0.0",
  "description":
    "Thanks to Hugo Wood (hgwood) regarding tooling used in this project",
  "main": "index.js",
  "scripts": {
    "start": "cross-env DEBUG=* node -r dotenv/config ./index",
    "lint": "eslint *.js",
    "prettier": "prettier --write \"*.js\"",
    "test": "mocha *.test.js",
    "init":
      "npm run download && git add *.pdf *.in.txt round.json package.json && git commit -m \":bento: add statement and input files\"",
    "download": "cross-env DEBUG=* node -r dotenv/config ./download",
    "upload": "cross-env DEBUG=* node -r dotenv/config ./upload",
    "zip": "cross-env DEBUG=* node -r dotenv/config ./zip",
    "scaffold": "node scaffold",
    "submit": "npm run zip && npm run upload",
    "all": "run-p input1 input2 input3 input4",
    "input1": "cross-env npm start ${npm_package_config_input1_name}.in.txt",
    "input2": "cross-env npm start ${npm_package_config_input2_name}.in.txt",
    "input3": "cross-env npm start ${npm_package_config_input3_name}.in.txt",
    "input4": "cross-env npm start ${npm_package_config_input4_name}.in.txt"
  },
  "config": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hgwood/hash-code-2019-qualification.git"
  },
  "keywords": ["hash code", "google", "contest", "2019"],
  "contributors": ["Maxime Claveau", "Patrice De Saint Steban"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hgwood/hash-code-2019-qualification/issues"
  },
  "homepage": "https://github.com/hgwood/hash-code-2019-qualification#readme",
  "engines": {
    "node": ">=8 <9"
  },
  "devDependencies": {
    "archiver": "^1.3.0",
    "eslint": "^4.18.1",
    "joi": "^10.2.0",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.10.2",
    "request": "^2.88.0",
    "request-promise": "^4.1.1"
  },
  "dependencies": {
    "cross-env": "^5.1.3",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "glob": "^7.1.1",
    "jolicitron": "^2.0.0",
    "lodash": "^4.17.11"
  }
}
